
 import "./project.zmodel"  

model RoleData {

    id        String    @id @default(uuid())  
    title     String      
    seniority String?      
    projectId String      
    project   Project?   @relation(fields: [projectId], references: [id], name:"project")  

dateCreated DateTime @default(now())
      dateUpdated DateTime @updatedAt  @default(now())
      @@allow("all", true)
}